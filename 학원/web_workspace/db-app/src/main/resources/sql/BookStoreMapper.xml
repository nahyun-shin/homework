<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="it.korea.mapper.BookStoreMapper">
 	
 	<select id="getAllBookStore" resultType="book">
 	
 		select *
 		from book_store
 		order by book_id desc
 		
 	</select>
 	
 	<select id="getBookStore" parameterType="map" resultType="book">
 	
 		select *
 		from book_store
 		where book_id = #{bookId}
 		
 	</select>
 
 </mapper>