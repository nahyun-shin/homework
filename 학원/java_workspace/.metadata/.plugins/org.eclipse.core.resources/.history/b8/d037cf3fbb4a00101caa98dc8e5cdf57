package test02.data;

	/**
	 * 계좌정보 클래스
	 * 계좌 정보를 담는 클래스
	 */
public class Infor {
	
	/**
	 * 고객계좌번호
	 */
	private int cashNum;
	/**
	 * 통장입출금
	 */
	private int cashInOut;
	
	/**
	 * 통장잔고
	 */
	private int cashSum;
	
	
		
	

	public int getCashNum() {
		return cashNum;
	}
	public void setCashNum(int cashNum) {
		this.cashNum = cashNum;
	}
	public int getCashInOut() {
		return cashInOut;
	}
	public void setCashInOut(int cashInOut) {
		this.cashInOut = cashInOut;
	}
	public int getCashSum() {
		return cashSum;
	}
	public void setCashSum(int cashSum) {
		this.cashSum = cashSum;
	}
	
	
	
	
	public int getTotal() {
		return this.getCashSum()+this.cashInOut;
	}
	public int getMinu() {
		return this.getCashSum()-this.cashInOut;
	}
	
	
	public String toString() {
		StringBuilder sb = new StringBuilder();
		sb.append("계좌번호 : "+this.getCashNum()+", ");
		sb.append("통장잔액 : "+this.getCashSum()+", ");
		return sb.toString();
	}
	
	
	
	
	
	
	
}
